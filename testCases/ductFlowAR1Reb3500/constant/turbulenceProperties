FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          turbulenceProperties;
}

simulationType  RAS;

RAS
{
    RASModel        kOmegaSSTPDA; 
    secondaryMode   2;          //0:off | 1:ModelI | 2:ModelII
    separationMode  3;          //0:off | 1:ModelI | 2:ModelII | 3:ModelIII | 4:ModelIV
}



