FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          turbulenceProperties;
}

simulationType  RAS;

RAS
{
    RASModel            kOmegaSSTPDA;

    //Separation coefficients - Optional
    separationMode      4;              
    secondaryMode       0;
    turbulence      on;
    printCoeffs     on;
}


